#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

build:

clean:
	dh_clean

binary-arch:

binary-indep:
	dh_installdocs
	dh_installchangelogs

	dh_installdirs
	dh_install

	dh_installdeb
	
	dh_installcron
	dh_link
	dh_installlogrotate
	
	dh_gencontrol
	dh_md5sums
	dh_builddeb

binary: binary-indep binary-arch

build-arch: build-stamp
build-indep: build-stamp

.PHONY: build clean binary-arch binary-indep binary
